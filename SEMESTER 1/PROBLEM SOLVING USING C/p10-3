#include <stdio.h>
void main()
{
 int n1 = 0, n2 = 1, n3, i, num,sum = 1;
 printf("Enter value: ");
 scanf("%d", &num);
 printf("Series is : %d %d", n1, n2);
 for (i = 2; i < num; i++)
   {
     n3 = n1 + n2;
     printf(" %d", n3);
     sum += n3;
     n1 = n2;
     n2 = n3;
   }
   printf("\nSum Of Series: %d",sum);
   printf("\n");
}
