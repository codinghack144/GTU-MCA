import java.util.Scanner;
public class Main
{
   public static void main(String[] args)
  {
   Scanner in=new Scanner(System.in);
   System.out.println("Enter base x");
   int x=in.nextInt();
   System.out.println("Enter exponenet n");
   int n=in.nextInt();
   int r=1;
   int i=1;
   while(i<=n)
   {
       r*=x;
	   i++;
   }
   System.out.println("\n Result::"+x+"^"+n+"="+r+"\n");
   }
}
