import java.util.Scanner;
class Palindrome
{
   public static void main(String[] args)
   {
	     int r,sum=0,temp,n;
	     System.out.print("Enter a number :");
       Scanner in=new Scanner(System.in);
       n=in.nextInt();
	temp=n;
	while(n>0)
    {
	      r=n%10;
	      sum=(sum*10)+r;
	      n=n/10;
	  }
	if(temp==sum)
	   System.out.println("Palindrome number");
	else
	   System.out.println("not Palindrome");
	}
}
